[Unit]
Description=Naval Unified IPsec ESP/AH Engine
After=network.target
Wants=network-online.target

[Service]
Type=simple
ExecStart=/usr/local/bin/esp_ah_controller
Restart=always
RestartSec=5
User=root
StandardOutput=append:/var/log/naval-ipsec.log
StandardError=append:/var/log/naval-ipsec.log

[Install]
WantedBy=multi-user.target
